  "description": "Source data from directus cms",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__ --ignore **/__fixtures__",
    "ci": "yarn install --frozen-lockfile",
    "watch": "babel -w src --out-dir . --ignore **/__tests__ --ignore **/__fixtures__"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "author": "Nicolas Venne <nicolas.maxvenne@gmail.com>",
  "license": "0BSD",
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@directus/sdk": "^9.7.1",
    "@vercel/fetch-retry": "^5.1.3",
    "axios": "^0.26.1",
    "gatsby-core-utils": "^3.12.0-next.0",
    "gatsby-plugin-utils": "^3.4.2",
    "gatsby-source-filesystem": "^4.12.0-next.0",
    "lodash": "^4.17.21",
    "node-fetch": "2",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
